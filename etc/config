; kernel image name
; use 'kernel_img' to override
;
; *** These are actually perl regexps! ***
;
[KernelImage]
default	= vmlinuz-
alpha	= vmlinuz
ppc	= vmlinux
ppc64	= vmlinux
mips	= vmlinux
s390	= image
s390x	= image


; kernel rpm name
; use 'kernel' to override
;
[KernelRPM]
default	= kernel-default
s390    = kernel-s390

; extra kmps we need (',' separated list)
[KMP]
default	=
i386	= ofed,xen,brocade-bna
x86_64	= ofed,xen,brocade-bna
ia64    = ofed
ppc     = ofed
ppc64   = ofed


; lib directory
[lib]
default	= lib
ppc64	= lib64
s390x	= lib64
x86_64	= lib64


